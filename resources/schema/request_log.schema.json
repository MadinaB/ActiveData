{
	"settings": {
		"index.number_of_replicas": 2,
		"index.number_of_shards": 3
	},
	"mappings": {
		"request_log": {
			"properties": {
				"path": {
					"type": "keyword"
				},
				"remote_addr": {
					"type": "keyword"
				},
				"http_accept_encoding": {
					"type": "keyword"
				},
				"query": {
					"type": "object",
					"enabled": false
				},
				"query_text": {
					"type": "text"
				},
				"data": {
					"type": "text"
				},
				"error": {
					"type": "object",
					"enabled": false
				},
				"from": {
					"type": "keyword"
				},
				"content_length": {
					"type": "long"
				},
				"http_user_agent": {
					"type": "keyword"
				},
				"timestamp":{
					"type": "double"
				}

			}

		}
	}
}

